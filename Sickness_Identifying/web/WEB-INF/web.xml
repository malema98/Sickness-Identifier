<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SymptoServ</servlet-name>
        <servlet-class>Controllers.SymptoServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ResultsServ</servlet-name>
        <servlet-class>Controllers.ResultsServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>User_RegisterServ</servlet-name>
        <servlet-class>Controllers.User_RegisterServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServ</servlet-name>
        <servlet-class>Controllers.LoginServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Appointment_BookingServ</servlet-name>
        <servlet-class>Controllers.Appointment_BookingServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Doctor_view_AppServ</servlet-name>
        <servlet-class>Controllers.Doctor_view_AppServ</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Doctor_Cancel_AppServ</servlet-name>
        <servlet-class>Controllers.Doctor_Cancel_AppServ</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SymptoServ</servlet-name>
        <url-pattern>/SymptoServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ResultsServ</servlet-name>
        <url-pattern>/ResultsServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>User_RegisterServ</servlet-name>
        <url-pattern>/User_RegisterServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServ</servlet-name>
        <url-pattern>/LoginServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Appointment_BookingServ</servlet-name>
        <url-pattern>/Appointment_BookingServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Doctor_view_AppServ</servlet-name>
        <url-pattern>/Doctor_view_AppServ</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Doctor_Cancel_AppServ</servlet-name>
        <url-pattern>/Doctor_Cancel_AppServ</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
